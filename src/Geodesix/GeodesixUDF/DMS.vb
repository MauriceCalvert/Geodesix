Imports Utilities
Partial Public Class GeodesixUDF

    Public Function DMS(value As Object) As Object

        Try
            Dim values As String = GetValueString(value)
            Return DMS2D(values)

        Catch ex As Exception
            Return ex.Message
        End Try

    End Function

End Class
