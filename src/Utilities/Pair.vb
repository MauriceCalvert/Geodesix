Public Class Pair

    Sub New(k As String, v As String)
        Key = k
        Value = v
    End Sub
    Public Property Key As String
    Public Property Value As String
    Public Overrides Function ToString() As String
        Return $"{Key}={Value}"
    End Function
End Class
