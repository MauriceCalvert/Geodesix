Imports System.Diagnostics

Public Module OpenBrowser_

    Public Sub OpenBrowser(url As String)

        Dim proc As New Process
        proc.StartInfo.UseShellExecute = True
        proc.StartInfo.FileName = url
        proc.Start()

    End Sub

End Module
