Public Module TryDouble_

    Public Function TryDouble(s As Object) As Object

        Dim result As Double

        If TypeOf s Is String AndAlso Double.TryParse(CStr(s), result) Then
            Return result
        Else
            Return s
        End If

    End Function

End Module
